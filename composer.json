{
    "name": "nitsan/ns-faq",
    "type": "typo3-cms-extension",
    "description": "This plugin provides a quick and easy way manage FAQs from backend and Frontend Plugin.",
    "homepage": "https://extensions.typo3.org/extension/ns_faq/",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "FAQ",
        "NITSAN"
    ],
    "support": {
        "issues": "https://github.com/nitsan-technologies/ns_comments/issues"
    },
    "require": {
        "typo3/cms-core": ">=9.0.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsFaq\\": "Classes/"
        }
    },
    "replace": {
        "nitsan/ns-faq": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_faq"
        }
    }
}